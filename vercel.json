{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon\\.(ico|svg)",
      "dest": "/favicon.$1"
    },
    {
      "src": "/QR\\.(jpeg|jpg|png)",
      "dest": "/QR.$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}